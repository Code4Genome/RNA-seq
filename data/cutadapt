#!/bin/bash
# LOAD MODULES HERE IF REQUIRED
ml cutadapt/1.18
# START THE APPLICATION
cutadapt -b GATCGGAAGAGCACACGTCTGAACTCCAGTCACTCCTGGACATCTCGTAT -b GATCGGAAGAGCACACGTCTGAACTCCAGTCACTCCTGGACATCGCGTAT -q 30,30 -m 20 -o B/H/50_92_S50_R1_001.fastq.gz
cutadapt -b GATCGGAAGAGCACACGTCTGAACTCCAGTCACGTTCTTATATCTCGTAT -b GATCGGAAGAGCACACGTCTGAACTCCAGTCACGTTCTTATATCGCGTAT -q 30,30 -m 20 -o B/M/99_116_S99_R1_001.fastq.gz 
cutadapt -b GATCGGAAGAGCACACGTCTGAACTCCAGTCACCAGTGACGATCTCGTAT -q 30,30 -m 20 -o B/M/trim_189_20.fastq.gz B/M/189.fastq.gz	
cutadapt -b GATCGGAAGAGCACACGTCTGAACTCCAGTCACCGAACGTGATCTCGTAT -q 30,30 -m 20 -o B/M/trim_54.fastq.gz B/M/54_96_S54.fastq.gz
cutadapt -b GATCGGAAGAGCACACGTCTGAACTCCAGTCACTACGATTAATCTCGTAT -b GATCGGAAGAGCACACGTCTGAACTCCAGTCACTACGATTAATCGCGTAT -q 30,30 -m 20 -o B/M/trim_32_64_001.fastq.gz 
